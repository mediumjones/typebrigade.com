script(src="//maps.googleapis.com/maps/api/js?key=AIzaSyDdIwbu42WYPH31NnxZ03XSMvuX9e7xKHg&amp;sensor=FALSE")
script.
  var map;
  var mobify = new google.maps.LatLng(49.2778192,-123.1194332);
  var marker;
  var mapOptions;
  function initialize() {
    mapOptions = {
      zoom: 14,
      center: mobify,
      disableDefaultUI: true,
      draggable: false,
      disableDoubleClickZoom: true,
      zoomControl: false,
      scrollwheel: false
    };
    map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);
    marker = new google.maps.Marker({
      position: mobify,
      map: map,
      title: 'Mobify'
    });
  };
  WebFontConfig = {
    timeout: 2000,
    typekit: {
      id: '#{ service.typekit }'
    },
    custom: {
      families: ['Edmondsans:n4,n5,n7'],
      urls: ['/css/fonts.css']
    },
    inactive: function(){
      initialize();
    },
    active: function(){
      initialize();
    }
  };
script(src="//ajax.googleapis.com/ajax/libs/webfont/1.5.3/webfont.js")
